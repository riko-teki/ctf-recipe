{
    "containers": {
        "ctf-recipe": {
            "image": "",
            "ports": {
                "80": "HTTPS"
            }
        }
    },
    "publicEndpoint": {
        "containerName": "ctf-recipe",
        "containerPort": 80,
        "healthCheck": {
            "healthyThreshold": 2,
            "unhealthyThreshold": 2,
            "timeoutSeconds": 3,
            "intervalSeconds": 5,
            "path": "/",
            "successCodes": "200-499"
        }
    }
}

